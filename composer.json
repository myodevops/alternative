{
    "name": "myodevops/alternative",
    "description": "Evolutions of layouts and components of Laravel AdminLTE",
	"license": "MIT",
    "version": "00.03.00",
	"authors": [
        {
            "name": "nicola-myo",
            "email": "n.cainelli@myobject.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "myodevops\\ALTErnative\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "myodevops\\ALTErnative\\ALTErnativeServiceProvider"
            ]
        },
        "public-files": [
            {
                "from": "js/alternative.js",
                "to": "public/js/alternative.js"
            },
            {
                "from": "css/alternative.css",
                "to": "public/css/alternative.css"
            }
        ]
    },
    "require": {
        "laravel/framework": ">=6.0",
        "php": ">=7.2.0",
        "jeroennoten/laravel-adminlte": ">=v3.8.5",
        "moment/moment": "^2.29.1",
        "datatables/datatables": "^1.10",
        "tempusdominus/bootstrap-4": "^5.1",
        "select2/select2": "^4.0"
    },
    "require-dev": {
        "phpunit/php-timer": "^5.0"
    }
}